
<core:FragmentDefinition xmlns="sap.m" xmlns:unified="sap.ui.unified" xmlns:core="sap.ui.core">
    <TableSelectDialog
        id="sortT1AscOptions"
        class="sapUiContentPadding"
        contentWidth="50%"
        contentHeight="60%"
        title="Country"
        liveChange="CurSearch"
        items="{/xNAUTIxCountrySetFetch}"
        cancel="handleValueHelpClose"
        confirm="handleValueHelpClose"
        initialFocus="SearchField"
         search="CurSearch"
        >
     
                <columns>
                    <Column >
                        <Text text="Country Code"></Text>
                    </Column>
 
                    <Column >
                        <Text text="Description"></Text>
                    </Column>
                </columns>
                <items>
                    <ColumnListItem >
                        <Text text="{land1}"></Text>
                        <Text text="{landx50}"></Text>
                    </ColumnListItem>
                </items>
               
     
 
        </TableSelectDialog>
</core:FragmentDefinition>