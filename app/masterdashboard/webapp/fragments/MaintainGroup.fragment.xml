
<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">  
    <SelectDialog
        id="mySelect"
        noDataText="Loading..."
        title="Select Users"
       search="onMaintainUserSearch"
       liveChange="onMaintainUserSearch"
        confirm="onMaintaingroupHelpClose"
        cancel="onMaintaingroupHelpClose"
         initialFocus="SearchField"
        items="{/xNAUTIxSAPUSERS}"
       
        >
        <StandardListItem id="_IDGenStand"
       
            title="{bname}"
           
         
       
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>