
<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"> 
    <SelectDialog
        id="mySelectDialogvoyage"
        noDataText="Loading..."
        title="Select Charter"
        search="onValueHelpSearchApprove"
        confirm="onValueHelpCloseChar"
        cancel="onValueHelpCloseChar"
        items="{/xNAUTIxCHARTERVALUEHELP}"
        multiSelect="true"
        initialFocus="SearchField"
        liveChange="onValueHelpSearchApprove"
        >
            
        <StandardListItem id="_IDGenStandardListItem1"
            title="{Chrnmin}"
       
            description="{Creqno}"
       
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>